{
    "name": "@shortlet-lagos/api",
    "version": "1.0.0",
    "description": "Shortlet-Lagos Application",
    "main": "src/server.ts",
    "engines": {
        "node": ">=17.0.0"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "nodemon ./src/server.ts",
        "dev": "cross-env NODE_ENV=development nodemon ./src/server.ts",
        "debug": "cross-env NODE_ENV=development nodemon ts-node-dev --respawn --transpile-only --project tsconfig.json ./src/server.ts",
        "dev:render": "node ./dist/server.js",
        "prod": "cross-env NODE_ENV=production node ./dist/server.js",
        "build": "tsc --project tsconfig.json",
        "lint": "eslint . --ext .ts --fix",
        "db:makemigration": "npx sequelize-cli db:migrate",
        "db:undomigration": "ts-node ./node_modules/.bin/sequelize-cli db:migrate:undo",
        "db:newmigration": "npx sequelize-cli migration:generate --name",
        "db:rename": "ts-node ./src/config/renameMigration.ts",
        "seed": "npx sequelize-cli db:seed:all",
        "down": "npx sequelize-cli db:migrate:undo"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@faker-js/faker": "^9.1.0",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.3",
        "cloudinary": "^1.41.3",
        "cookie-parser": "^1.4.6",
        "cookie-session": "^2.1.0",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^16.4.5",
        "ejs": "^3.1.10",
        "eslint": "^8.57.0",
        "ethers": "^6.13.3",
        "express": "^4.20.0",
        "express-async-errors": "^3.1.1",
        "express-mongo-sanitize": "^2.2.0",
        "express-winston": "^4.2.0",
        "helmet": "^7.1.0",
        "ioredis": "^5.4.1",
        "jsonwebtoken": "^9.0.2",
        "moment": "^2.30.1",
        "moment-timezone": "^0.5.45",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.15",
        "nodemon": "^3.1.4",
        "pg": "^8.12.0",
        "pg-hstore": "^2.3.4",
        "puppeteer": "^22.13.0",
        "sequelize": "^6.37.3",
        "sequelize-cli": "^6.6.2",
        "sequelize-typescript": "^2.1.6",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.6.2",
        "uuid": "^9.0.1",
        "winston": "^3.14.2",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cookie-parser": "^1.4.7",
        "@types/cookie-session": "^2.0.49",
        "@types/cors": "^2.8.17",
        "@types/ejs": "^3.1.5",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.12",
        "@types/node": "^20.16.5",
        "@types/nodemailer": "^6.4.15",
        "@types/sequelize": "^4.28.20",
        "@types/uuid": "^9.0.8",
        "@types/validator": "^13.12.1",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "ts-node": "^10.9.2"
    },
    "keywords": []
}
